{
  "family": "mindscribe-frontend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::699634487278:role/mindscribe-ecs-execution-role",
  "taskRoleArn": "arn:aws:iam::699634487278:role/mindscribe-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "699634487278.dkr.ecr.us-east-1.amazonaws.com/mindscribe/frontend:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/mindscribe-frontend",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "frontend",
          "awslogs-create-group": "true"
        }
      },
      "healthCheck": {
        "command": ["CMD-SHELL", "wget --quiet --tries=1 --spider http://localhost/ || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 10
      }
    }
  ]
}
